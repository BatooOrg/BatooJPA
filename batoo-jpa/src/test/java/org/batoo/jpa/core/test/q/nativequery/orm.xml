<entity-mappings xmlns="http://java.sun.com/xml/ns/persistence/orm" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/persistence/orm http://java.sun.com/xml/ns/persistence/orm_2_0.xsd" version="2.0">

<named-native-query name="namedNativeQuery3" result-set-mapping="OrderItemResults">
    <query>![CDATA[SELECT o.id,  o.quantity, o.item_id,  i.id, 'itemX' as name, i.description  FROM ORDER_T o, Item i WHERE (o.quantity > ?) AND (o.item_id = i.id)]></query>
</named-native-query>
                 
</entity-mappings>